# Visual Gram Lab - Environment Variables Example
# Copia questo file come .env.local e configura le tue credenziali

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Optional: Service Role Key (solo per operazioni admin)
# VITE_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# App Configuration
VITE_APP_NAME=Visual Gram Lab
VITE_APP_VERSION=1.0.0

# Development Settings
VITE_DEBUG=false
VITE_LOG_LEVEL=info

# Feature Flags (opzionale)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# External Services (opzionale)
# VITE_SENTRY_DSN=your-sentry-dsn
# VITE_GOOGLE_ANALYTICS_ID=your-ga-id
